{
  "name": "genetic-constructor",
  "version": "0.11.1",
  "description": "Genetic Constructor",
  "main": "index.js",
  "scripts": {
    "start": "./node_modules/.bin/babel-node tools/run start",
    "start-auth": "./node_modules/.bin/babel-node tools/run auth-stack",
    "start-instance": "./node_modules/.bin/babel-node tools/run start-instance",
    "start-docker": "docker build -t genome-designer . && docker run --privileged -v /var/run/docker.sock:/run/docker.sock -i -p 3000:3000 -t genome-designer",
    "start-db": "./node_modules/.bin/babel-node tools/run startDb",
    "server-auth": "EMAIL=1 COMMAND='npm run start-instance -- --release' ./bin/server-auth.sh",
    "postinstall": "npm run build-classes && npm run install-dependencies && npm run install-extensions && npm run selenium && npm run jsdoc",
    "build": "./node_modules/.bin/babel-node tools/run build",
    "build-client": "./node_modules/.bin/babel-node tools/run bundle",
    "build-classes": "./node_modules/.bin/babel-node tools/run bundleClasses",
    "deploy-classes": "npm run build-classes && git subtree push --prefix constructor-classes https://github.com/maxbates/constructor-classes.git master",
    "install-extensions": "./node_modules/.bin/babel-node tools/run installExtensions",
    "install-dependencies": "./node_modules/.bin/babel-node tools/run install-dependencies",
    "jsdoc": "jsdoc -c docs/jsdoc/jsdoc.conf.json --package package.json --readme docs/jsdoc/jsdoc.md",
    "check": "./node_modules/.bin/babel-node tools/run checks && npm run lint && npm run test",
    "lint": "./node_modules/eslint/bin/eslint.js src server tools commons --quiet",
    "test": "NODE_ENV=test ./node_modules/.bin/babel-node tools/run test",
    "test-coverage": "COVERAGE=true npm run test",
    "test-coverage-report": "COVERAGE=true REPORT=true npm run test",
    "test-jenkins": "JENKINS=true npm run test",
    "artillery": "chmod u+x test-load/test-load.sh && test-load/test-load.sh",
    "nightwatch": "node ./node_modules/nightwatch/bin/nightwatch --retries 1 --config ./test-e2e/nightwatch.js --env local",
    "nightwatch-saucelabs": "node ./node_modules/nightwatch/bin/nightwatch --config ./test-e2e/nightwatch.js --env saucelabs",
    "e2e": "NODE_ENV=test node ./bin/e2e.js",
    "imagediff": "node ./bin/imagediff/imagediff.js --truth ./test-e2e/cannonical-screenshots/ --hypothesis ./test-e2e/current-screenshots/",
    "selenium": "node ./node_modules/selenium-standalone/bin/selenium-standalone install --version=2.53.1 --drivers.chrome.version=2.24"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Autodesk/genetic-constructor.git"
